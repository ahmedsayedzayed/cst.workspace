{
  "name": "portal",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/portal/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/portal",
        "index": "apps/portal/src/index.html",
        "main": "apps/portal/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/portal/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/portal/src/favicon.ico",
          "apps/portal/src/assets"
        ],
        "styles": [
          {
            "input": "apps/portal/src/assets/styles/css/light.css",
            "inject": false,
            "bundleName": "light"
          },
          {
            "input": "apps/portal/src/assets/styles/css/light.css",
            "inject": false,
            "bundleName": "dark"
          },
          {
            "input": "apps/portal/src/assets/styles/css/bootstrap-light.css",
            "inject": false,
            "bundleName": "bootstrap-light"
          },
          {
            "input": "apps/portal/src/assets/styles/css/bootstrap-light.css",
            "inject": false,
            "bundleName": "bootstrap-dark"
          },
          {
            "input": "apps/portal/src/assets/styles/css/ng-bundle.css",
            "inject": false,
            "bundleName": "ng-bundle"
          },
          {
            "input": "apps/portal/src/assets/styles/css/layout-bundle.css",
            "inject": false,
            "bundleName": "layout-bundle"
          },
          {
            "input": "node_modules/@volosoft/abp.ng.theme.lepton-x/assets/css/abp-bundle.css",
            "inject": false,
            "bundleName": "abp-bundle"
          },
          {
            "input": "apps/portal/src/assets/styles/css/light.rtl.css",
            "inject": false,
            "bundleName": "light.rtl"
          },
          {
            "input": "apps/portal/src/assets/styles/css/light.rtl.css",
            "inject": false,
            "bundleName": "dark.rtl"
          },
          {
            "input": "apps/portal/src/assets/styles/css/bootstrap-light.rtl.css",
            "inject": false,
            "bundleName": "bootstrap-light.rtl"
          },
          {
            "input": "apps/portal/src/assets/styles/css/bootstrap-light.rtl.css",
            "inject": false,
            "bundleName": "bootstrap-dark.rtl"
          },
          {
            "input": "apps/portal/src/assets/styles/css/font-bundle.css",
            "inject": false,
            "bundleName": "font-bundle"
          },
          {
            "input": "apps/portal/src/assets/styles/css/font-bundle.rtl.css",
            "inject": false,
            "bundleName": "font-bundle.rtl"
          },
          {
            "input": "apps/portal/src/assets/styles/css/ng-bundle.rtl.css",
            "inject": false,
            "bundleName": "ng-bundle.rtl"
          },
          {
            "input": "apps/portal/src/assets/styles/css/layout-bundle.rtl.css",
            "inject": false,
            "bundleName": "layout-bundle.rtl"
          },
          {
            "input": "node_modules/@volosoft/abp.ng.theme.lepton-x/assets/css/abp-bundle.rtl.css",
            "inject": false,
            "bundleName": "abp-bundle.rtl"
          },
          "node_modules/bootstrap-icons/font/bootstrap-icons.css",
          "apps/portal/src/styles.scss",
          "node_modules/ngx-spinner/animations/ball-scale-multiple.css",
          "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
          "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/portal/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "100kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/portal/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:module-federation-dev-server",
      "options": {
        "port": 4200,
        "publicHost": "http://localhost:4200"
      },
      "configurations": {
        "production": {
          "buildTarget": "portal:build:production"
        },
        "development": {
          "buildTarget": "portal:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "portal:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}